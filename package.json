{
  "name": "batman",
  "version": "1.0.0",
  "description": "UI Components in vanilla js (TypeScript)",
  "main": "index.html",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "tsc && concurrently \"scss src/sass:src/css --watch\" \"scss demo/sass:demo/css --watch\" \"tsc -w\" \"lite-server\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "keywords": [
    "batman"
  ],
  "author": "Aravind Kumar P",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.4.0",
    "lite-server": "^2.3.0"
  }
}
