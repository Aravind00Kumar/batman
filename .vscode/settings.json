{
	"typescript.tsdk": "./node_modules/typescript/lib/",
	"files.exclude": {
		"**/*.js": {
			"when": "$(basename).ts"
		},
		//"**/*.map": true
	},
	"vsicons.projectDetection.disableDetect": true,
	"vsicons.associations.files": [
		{
			"icon": "webpack",
			"extensions": [
				"webpack.config.components.umd.js",
				"webpack.config.core.umd.js",
				"webpack.config.multi.js",
				"webpack.config.single.js",
				"webpack.config.components.umd.prod.js",
				"webpack.config.core.umd.prod.js",
				"webpack.config.multi.prod.js",
				"webpack.config.single.prod.js",
				"webpack.config.aio.umd.js",
				"webpack.config.aio.umd.prod.js"
			],
			"filename": true,
			"format": "svg"
		},
		{
			"icon": "tsconfig",
			"extensions": [
				"tsconfig.components.json",
				"tsconfig.core.json",
				"tsconfig.multi.json",
				"tsconfig.single.json",
				"tsconfig.aio.json"
			],
			"filename": true,
			"format": "svg"
		}
	]
}